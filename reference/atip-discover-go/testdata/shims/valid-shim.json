{
  "atip": {
    "version": "0.4"
  },
  "name": "curl",
  "version": "8.4.0",
  "description": "Transfer data from or to a server",
  "homepage": "https://curl.se",
  "trust": {
    "source": "community",
    "verified": false
  },
  "commands": {
    "": {
      "description": "Make an HTTP request",
      "options": [
        {
          "name": "request",
          "flags": ["-X", "--request"],
          "type": "enum",
          "enum": ["GET", "POST", "PUT", "DELETE", "PATCH", "HEAD", "OPTIONS"],
          "default": "GET",
          "description": "HTTP method to use"
        },
        {
          "name": "header",
          "flags": ["-H", "--header"],
          "type": "string",
          "description": "Custom header to include"
        },
        {
          "name": "data",
          "flags": ["-d", "--data"],
          "type": "string",
          "description": "Request body data"
        },
        {
          "name": "output",
          "flags": ["-o", "--output"],
          "type": "string",
          "description": "Write output to file"
        }
      ],
      "effects": {
        "network": true,
        "idempotent": false,
        "destructive": false,
        "reversible": true
      }
    }
  }
}
